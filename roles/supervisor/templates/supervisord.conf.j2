[unix_http_server]
file=/home/{{ketadb_user}}/supervisor/supervisor.sock           ; the path to the socket file

[supervisord]
logfile=/home/{{ketadb_user}}/supervisor/supervisor.log         ; main log file; default $CWD/supervisord.log
logfile_maxbytes=50MB               ; max main logfile bytes b4 rotation; default 50MB
logfile_backups=3                   ; # of main logfile backups; 0 means none, default 10
loglevel=info                       ; log level; default info; others: debug,warn,trace
pidfile=/home/{{ketadb_user}}/supervisor/supervisor.pid         ; supervisord pidfile; default supervisord.pid
nodaemon=false                      ; start in foreground if true; default false
minfds=1024                         ; min. avail startup file descriptors; default 1024
minprocs=200                        ; min. avail process descriptors;default 200

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix:///home/{{ketadb_user}}/supervisor/supervisor.sock ; use a unix:// URL  for a unix socket

[include]
files = /home/{{ketadb_user}}/supervisor/conf.d/*.conf
