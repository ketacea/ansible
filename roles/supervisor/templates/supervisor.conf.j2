[unix_http_server]
file={{ supervisor_sockfile }}

[supervisord]
pidfile={{ supervisor_pidfile }}
logfile={{ supervisor_logfile }}
logfile_maxbytes={{ supervisor_logfile_maxbytes }}
logfile_backups={{ supervisor_logfile_backups }}
loglevel={{ supervisor_loglevel }}
nodaemon=true
minfds=1024
minprocs=200

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisorctl]
serverurl=unix://{{ supervisor_sockfile }}

[include]
files=/opt/ksupervisor/conf.d/*.conf
